<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
						"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="true" />

	<package name="character" namespace="/" extends="struts-default">
		<action name="pcOverview"
			class="nl.delphinity.questlog.controllers.CharacterController"
			method="Execute">
			<result name="success">/jsp/pcOverview.jsp</result>
		</action>
		
		
		<action name="createCharacter"
			class="nl.delphinity.questlog.controllers.CharacterController"
			method="createCharacter">
			<result name="success">/jsp/pcOverview.jsp</result>
		</action>
		
		
	</package>
</struts>